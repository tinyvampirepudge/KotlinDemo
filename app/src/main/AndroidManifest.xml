<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.tongtong.tiny.kotlindemo1">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="首页">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".base.BasicSyntaxActivity"
            android:label="基本语法">
        </activity>
        <activity
            android:name=".recyclerview.RecyclerViewActivity"
            android:label="recyclerView">
        </activity>
        <activity
            android:name=".base.RequestApiActivity"
            android:label="请求api数据">
        </activity>
        <activity
            android:name=".data.DataActivity"
            android:label="数据类">
        </activity>
        <activity
            android:name=".data.ForecastListActivity"
            android:label="日期列表数据">
        </activity>
    </application>

</manifest>